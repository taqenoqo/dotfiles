{
  "https.rejectUnauthorized": false,
  "http.proxyStrictSSL": false,
  "suggest.noselect": true,
  "diagnostic.errorSign": "X",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "i",
  "diagnostic.displayByAle": true,
  "git.addedSign.hlGroup": "DiffAddSign",
  "git.changedSign.hlGroup": "DiffChangeSign",
  "git.removedSign.hlGroup": "DiffDeleteSign",
  "git.topRemovedSign.hlGroup": "DIffDeleteSign",
  "git.changeRemovedSign.hlGroup": "DiffDeleteSign",
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "formattingProvider": "stylish-haskell"
        }
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    } 
  },
  "markdownlint.config": {
    "ul-indent": {
      "indent": 4
    },
    "ul-style": {
      "style": "sublist"
    },
    "no-multiple-blanks": {
      "maximum": 2
    },
    "header-style": false,
    "fenced-code-language": false,
    "line-length": {
      "line_length": 120
    }
  },
  "cSpell.userWords": [
    "Jetpack",
    "Valkey",
    "fuga",
    "fullscreen",
    "hoge",
    "instanceof",
    "piyo",
    "typeof"
  ]
}
